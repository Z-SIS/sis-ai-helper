# Environment Variables Example
# ======================================
# Copy this file to .env.local and fill in the values
# These are the clean variable names that should be used in production

# Supabase Configuration (REQUIRED)
# ===================================

# Supabase Project URL
# Source: NEXT_PUBLIC_mrofgjydvwjqbnhxrits_SUPABASE_URL
NEXT_PUBLIC_SUPABASE_URL=

# Supabase Anonymous Key
# Source: NEXT_PUBLIC_mrofgjydvwjqbnhxrits_SUPABASE_ANON_KEY
NEXT_PUBLIC_SUPABASE_ANON_KEY=

# Supabase Service Role Key
# Source: mrofgjydvwjqbnhxrits_SUPABASE_SERVICE_ROLE_KEY
SUPABASE_SERVICE_ROLE_KEY=

# Supabase JWT Secret
# Source: mrofgjydvwjqbnhxrits_SUPABASE_JWT_SECRET
SUPABASE_JWT_SECRET=


# Database Configuration (REQUIRED)
# ===================================

# Database Connection URL (for Prisma)
# Source: mrofgjydvwjqbnhxrits_POSTGRES_URL
DATABASE_URL=

# PostgreSQL Connection URL
# Source: mrofgjydvwjqbnhxrits_POSTGRES_URL
POSTGRES_URL=

# PostgreSQL Prisma URL
# Source: mrofgjydvwjqbnhxrits_POSTGRES_PRISMA_URL
POSTGRES_PRISMA_URL=

# PostgreSQL Non-Pooling URL
# Source: mrofgjydvwjqbnhxrits_POSTGRES_URL_NON_POOLING
POSTGRES_URL_NON_POOLING=

# PostgreSQL Username
# Source: mrofgjydvwjqbnhxrits_POSTGRES_USER
POSTGRES_USER=

# PostgreSQL Password
# Source: mrofgjydvwjqbnhxrits_POSTGRES_PASSWORD
POSTGRES_PASSWORD=

# PostgreSQL Database Name
# Source: mrofgjydvwjqbnhxrits_POSTGRES_DATABASE
POSTGRES_DATABASE=

# PostgreSQL Host
# Source: mrofgjydvwjqbnhxrits_POSTGRES_HOST
POSTGRES_HOST=


# Authentication (Optional)
# ===========================

# Clerk Secret Key
CLERK_SECRET_KEY=

# Clerk Publishable Key
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=


# AI Services (Optional)
# ========================

# Google Generative AI API Key
GOOGLE_GENERATIVE_AI_API_KEY=

# Tavily Search API Key
TAVILY_API_KEY=

# ZAI API Key
ZAI_API_KEY=


# Platform Configuration
# ========================

NODE_ENV=development
VERCEL_ENV=development
VERCEL_URL=localhost:3000

# NOTES
# =====
# 1. For Vercel deployment, use the Vercel CLI to set environment variables
# 2. The clean variable names above should be used instead of the auto-generated ones
# 3. DATABASE_URL should be in postgresql:// format for Prisma compatibility
# 4. AI service keys are optional but recommended for full functionality

